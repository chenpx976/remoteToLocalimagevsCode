{
  "name": "remotetolocalimagevscode",
  "displayName": "remoteToLocalimagevsCode",
  "description": "",
  "version": "0.0.1",
  "engines": {
    "vscode": "^1.82.0"
  },
  "categories": [
    "Other"
  ],
  "activationEvents": [
    "onStartupFinished"
  ],
  "main": "./extension.js",
  "contributes": {
    "codeLensProvider" : {
      "resolveProvider": "true"
  },
    "commands": [
      {
        "command": "remotetolocalimagevscode.replaceinworkspaces",
