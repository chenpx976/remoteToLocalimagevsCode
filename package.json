    "eslint": "^7.27.0",
    "glob": "^7.1.7",
    "mocha": "^10.2.0",
    "typescript": "^4.3.2",
    "vscode-test": "^1.5.2"
  },
  "dependencies": {
    "axios": "^1.5.1",
